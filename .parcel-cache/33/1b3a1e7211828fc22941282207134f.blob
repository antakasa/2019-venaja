.venezuela-header {
  position: absolute;
  z-index: 34;
  /* margin-left: auto; */
  /* margin-right: auto; */
  display: -ms-flexbox;
  display: flex;
  width: 100%;
  -ms-flex-pack: center;
      justify-content: center;
  height: 70px;
  margin-top: 20px;
  -ms-flex-direction: column;
      flex-direction: column;
  -ms-flex-align: start;
      align-items: flex-start;
}

.venezuela-ul-logo {
  padding: 10px;
  max-width: 100px;
}



.venezuela-ul-logo.desktop {
  padding: 0;
  margin-top: 0px;
  height: 50px;
  width: auto !important;
  max-width: unset;
  margin-left: 40px;
  margin-top: 40px;
  position:relative;
  z-index: 2;
}

@media screen {
  @media (min-width: 1025px) { 
    .venezuela-header {
    margin-top: 0;
    }

    .venezuela-ul-logo.desktop {
      margin-top: 0;
      margin-left: 0;
      position: absolute;
      top: 27px;
      left: 30px;
    }
  }
}

.venezuela-logo-container {
  display: -ms-flexbox;
  display: flex;
  width: 100%;
  -ms-flex-pack: justify;
      justify-content: space-between;
  -ms-flex-align: end;
      align-items: flex-end;
}

.logo-and-instructions {
  display: -ms-flexbox;
  display: flex;
  width: 100%;
  -ms-flex-align: center;
  align-items: center;
  -ms-flex-pack: justify;
  justify-content: space-between;
}

.logo-and-instructions div {
  color: white;
  margin-right: 10px;
  text-transform: uppercase;
  background: black;
  border-radius: 100%;
  padding-right: 5px;
  border: 0px solid #244a4f;
  padding: 10px;
}

.logo-and-instructions div svg {
  width: 30px;
  height: 30px;
}

@media only screen and (min-width: 1250 px) {
  venezuela-header {
    display: none;
  }
}
